{
  "name": "nexustrade",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "test:system": "./scripts/test-system.sh",
    "docs:generate": "node scripts/generate-api-docs.js",
    "docker:build": "docker build -t nexustrade:latest .",
    "docker:run": "docker run --rm -p 3000:3000 nexustrade:latest",
    "docker:compose": "docker-compose up -d",
    "docker:compose:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:compose:down": "docker-compose down",
    "docker:autostart": "./scripts/docker-autostart.sh",
    "docker:status": "docker-compose ps && echo '\\nüåê Services:' && echo 'NexusTrade: http://localhost:3000' && echo 'Health Check: http://localhost:3000/health'",
    "test:integration:setup": "docker-compose -f docker-compose.test.yml up -d",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:teardown": "docker-compose -f docker-compose.test.yml down",
    "test:auth": "jest --config jest.auth.config.js",
    "test:auth:watch": "jest --config jest.auth.config.js --watch",
    "test:auth:coverage": "jest --config jest.auth.config.js --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "axios": "^1.6.2",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "http-proxy-middleware": "^3.0.5",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.4",
    "jsdom": "^22.1.0",
    "mongodb-memory-server": "^9.1.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.1.0",
    "node-fetch": "^2.7.0",
    "winston": "^3.17.0",
    "ws": "^8.18.2",
    "xml2js": "^0.6.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ]
  }
}
